+
+    int32_t ret;
+    uint32_t offset_buf[4];
+
+    DRPAI_DEBUG_PRINT(KERN_INFO "%s: (pid %d)\n", __func__, current->pid);
+
+    if (DRP_CH_NUM <= ch)
